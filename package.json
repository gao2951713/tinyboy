{
  "name": "tinyboy",
  "version": "1.0.0",
  "description": "this is a tiny project of reactjs and nodejs",
  "main": "index.js",
  "scripts": {
    "build": "browserify --debug  app.js > bundle.js",
    "build-dist": "NODE_ENV=production browserify index.js | uglifyjs -m  > bundle.min.js",
    "watch": "watchify --debug src/* -o dist/",
    "demon": "nodemon --debug --watch src --watch libs"
  },
  "keywords": [
    "demo"
  ],
  "author": "VG",
  "license": "ISC",
  "dependencies": {
  },
  "babel": {
    "presets": [
      "react"
    ]
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "literalify": "^0.4.0",
    "nodemon": "^1.11.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "uglify": "^0.1.5",
    "watchify": "^3.7.0",
    "babel-core": "^6.18.2",
    "babel-standalone": "^6.18.2"
  }
}
